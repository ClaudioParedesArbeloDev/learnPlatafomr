<header>
  <div class="title">
    <img src="/img/fullstack.svg" alt="logo" />
    <div>
      <h1>Claudio Paredes Arbelo</h1>
      <p>Desarrollador Fullstack y Educador</p>
    </div>
  </div>
  <nav>
    <div class="buttons">
      <a href="/"><button class="btn">home</button></a>
      <a href="/cursos"><button class="btn">cursos</button></a>
      <a href="/sobremi"><button class="btn">sobre mi</button></a>
      <a href="/contacto"><button class="btn">contacto</button></a>
    </div>
  </nav>
</header>
<section class="individualPage">
<div class="wrapperIndividual">
<img src="/{{avatar}}" alt="avatar" class="avatar">
  <div class="tablaGeneral">
  
    <div class="encabezadosIndividuo">
      <p>nombre:</p>
      <p>apellido:</p>
      <p>nro documento:</p>
      <p>domicilio:</p>
      <p>nro celular:</p>
      <p>fecha de nacimiento:</p>
      <p>e-mail:</p>
    </div>
    <div class="datosIndividuo">
      <p>{{nombre}}</p>
      <p>{{apellido}}</p>
      <p>{{dni}}</p>
      <p>{{domicilio}}</p>
      <p>{{celular}}</p>
      <p>{{fechaNacimiento}}</p>
      <p>{{email}}</p>
    </div>
   
  </div>
<button class="frenteDNI"><a href="">DNI Frente</a></button>
<button class="dorsoDNI"><a href="">DNI Dorso</a></button>



<button class="deleteBTN"><a href="/individual/delete/{{dni}}">Eliminar</a></button>

</div>

</section>

  <section class="modalDNIfrente">
        <div class="modal__containerDNIfrente">
          <a href="#" class="modal__closeFrente"><i class="fa-solid fa-xmark"></i></a>
            
            <p class="modal__title">DNI frente de {{nombre}} {{apellido}}</p>
            <img src="/{{frente}}" alt="" class="imgModal">
          </div>
    </section>

    <section class="modalDNIdorso">
        <div class="modal__containerDNIdorso">
          <a href="#" class="modal__closeDorso"><i class="fa-solid fa-xmark"></i></a>
            
            <p class="modal__title">DNI dorso de {{nombre}} {{apellido}}</p>
            <img src="/{{dorso}}" alt="" class="imgModal">
          </div>
    </section>

<div class="wrapperBtnListado">
    <button class="btnListado"><a href="/listado">listado</a></button>
</div>
<script src="/js/individual.js"></script>